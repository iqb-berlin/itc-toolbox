<Page x:Class="LoadDataFromTestcenterPage4Results"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:libco="clr-namespace:iqb.lib.components;assembly=iqblib-components"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="516"
      Title="Ergebnisse">
    <Page.Resources>
        <ResourceDictionary Source="pack://application:,,,/iqblib-components;component/GlobalStyles.xaml"/>
    </Page.Resources>

    <DockPanel>
        <ItemsControl Style="{StaticResource gsDialogBoxFooter}" DockPanel.Dock="Bottom" ItemContainerStyle="{StaticResource gsDialogBoxFooterButton}" Panel.ZIndex="2">
            <Button Name="BtnCancelClose" Click="BtnCancelClose_Click" DockPanel.Dock="Bottom" 
                HorizontalAlignment="Center" TabIndex="4">Schließen</Button>
        </ItemsControl>
        <libco:AsynchProgressBarUserControl Name="APBUC" Margin="5 0 5 15" DockPanel.Dock="Bottom"/>
        <libco:MessageBoxUserControl Name="MBUC" Margin="5"/>
    </DockPanel>
</Page>
